{
  "name": "minos",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "react-scripts": "3.4.4"
  },
  "dependencies": {
    "@emotion/core": "10.1.1",
    "@emotion/styled": "^10.0.27",
    "@mdi/js": "5.9.55",
    "@mdi/react": "^1.4.0",
    "@types/d3-path": "1.0.9",
    "@types/d3-scale": "2.2.4",
    "@types/jest": "^25.2.3",
    "@types/lodash-es": "4.17.4",
    "@types/node": "14.14.25",
    "@types/react": "16.14.3",
    "@types/react-dom": "16.9.10",
    "@types/react-modal": "3.12.0",
    "@types/tinycolor2": "^1.4.2",
    "d3-path": "^1.0.9",
    "d3-scale": "3.2.3",
    "emotion": "^10.0.27",
    "focus-visible": "5.2.0",
    "lodash-es": "4.17.20",
    "panzoom": "9.4.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-modal": "3.12.1",
    "react-router": "6.0.0-beta.0",
    "react-router-dom": "6.0.0-beta.0",
    "sanitize.css": "^11.0.1",
    "tinycolor2": "1.4.2",
    "typescript": "3.9.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(lodash-es))/\"",
    "eject": "react-scripts eject",
    "prettier": "prettier 'src/**/*.{json,css}'",
    "eslint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:other": "yarn prettier --check",
    "lint:code": "yarn eslint",
    "lint": "yarn lint:other && yarn lint:code",
    "format:other": "yarn prettier --write",
    "format:code": "yarn eslint --fix",
    "format": "yarn format:other && yarn format:code"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{ts,tsx,json,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
