{
  "name": "minos",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5",
    "react-scripts": "^3.4.1"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@mdi/js": "^5.3.45",
    "@mdi/react": "^1.4.0",
    "@types/d3-path": "^1.0.8",
    "@types/d3-scale": "^2.2.0",
    "@types/jest": "^25.2.3",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.0.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.10.5",
    "@types/tinycolor2": "^1.4.2",
    "d3-path": "^1.0.9",
    "d3-scale": "^3.2.1",
    "emotion": "^10.0.27",
    "focus-visible": "^5.1.0",
    "lodash-es": "^4.17.15",
    "panzoom": "^9.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-router": "6.0.0-alpha.5",
    "react-router-dom": "6.0.0-alpha.5",
    "sanitize.css": "^11.0.1",
    "tinycolor2": "^1.4.1",
    "typescript": "^3.9.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(lodash-es))/\"",
    "eject": "react-scripts eject",
    "prettier": "prettier 'src/**/*.{json,css}'",
    "eslint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:other": "yarn prettier --check",
    "lint:code": "yarn eslint",
    "lint": "yarn lint:other && yarn lint:code",
    "format:other": "yarn prettier --write",
    "format:code": "yarn eslint --fix",
    "format": "yarn format:other && yarn format:code"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{ts,tsx,json,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
