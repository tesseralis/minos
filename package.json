    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "react-scripts": "^4.0.3"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@types/d3-path": "^2.0.0",
    "@types/d3-scale": "^3.2.2",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.0.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/tinycolor2": "^1.4.2",
    "d3-path": "^1.0.9",
    "d3-scale": "^3.2.1",
    "history": "^5.0.0",
    "lodash": "^4.17.21",
    "next": "^10.1.3",
    "panzoom": "^9.2.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sanitize.css": "^11.0.1",
    "tinycolor2": "^1.4.1",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "test": "jest --watch",
    "prettier": "prettier '**/*.{json,css}'",
    "eslint": "eslint '**/*.{ts,tsx}'",
    "lint:other": "yarn prettier --check",
    "lint:code": "yarn eslint",
    "lint": "yarn lint:other && yarn lint:code",
    "format:other": "yarn prettier --write",
    "format:code": "yarn eslint --fix",
    "format": "yarn format:other && yarn format:code"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{ts,tsx,json,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
